{
  "compilerOptions": {
    "target": "es2020",
    "moduleResolution": "bundler",
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "outDir": "dist/types",
    "declaration": true,
    "emitDeclarationOnly": true,
    "removeComments": false,
    "skipLibCheck": true
  },
  "include": [
    "src/scramble-display"
  ]
}
